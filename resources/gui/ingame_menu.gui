[INGAME_MENU] {
    layout: vertical
    alignment: right

    [END_TURN] {
        @ref: BUTTON
        caption: End turn
        {
            type: keynavigator
            select: A
            position: -8, 0
            size: 16, 16
            visible: true
        }
    }
    [BACK] {
        @ref: BUTTON
        caption: Back____
        keybinding: B
    }
    [EXIT] {
        @ref: BUTTON
        caption: Quit____
    }
}

[@BUTTON] {
    type: button
    size: 80, 16
}
